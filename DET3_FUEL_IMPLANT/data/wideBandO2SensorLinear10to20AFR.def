// scale definition
name      = WIDE BAND SENSOR AFR 10-20


linearize = 0
unit      = AFR

minValue  = 10
maxValue  = 20 

// ilosc miejsc po przecinku
precision = 1

// non linear sensors are defined by equation:
// value = coef5 * v^5 +  coef4 * v^4 + coef3 * v^3 + coef2 * v^2 + coef1 * v + coef0
coef0 = 10
coef1 = 2
coef2 = 0
coef3 = 0
coef4 = 0
coef5 = 0
